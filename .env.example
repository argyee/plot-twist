# Environment (development or production)
NODE_ENV=development

# Bot Language (en for English, el for Greek)
LANGUAGE=en

# Discord Bot Token
# Get these values from here https://discord.com/developers/applications
DISCORD_TOKEN=

# TMDB API Key from themoviedb.org
TMDB_API_KEY=

# Discord Forum Channel ID (right-click channel → Copy ID)
MOVIE_FORUM_CHANNEL_ID=

# TMDB Image Base URL
TMDB_IMAGE_BASE_URL=https://image.tmdb.org/t/p/w500

# Overseerr Integration (optional)
# Base URL for your Overseerr instance (e.g., http://localhost:5055)
OVERSEERR_URL=

# Overseerr API Key (Settings → General → API Key)
OVERSEERR_API_KEY=
